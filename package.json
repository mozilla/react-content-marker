{
  "name": "react-content-marker",
  "version": "2.0.0",
  "description": "Content Marker for React",
  "repository": "github:mozilla/react-content-marker",
  "author": "Adrian Gaudebert <adrian@gaudebert.fr>",
  "license": "BSD-3-Clause",
  "keywords": [
    "react",
    "marker",
    "highlight",
    "mark",
    "term",
    "text",
    "word",
    "match",
    "occurence",
    "substring"
  ],
  "main": "build/index.js",
  "types": "build/index.d.js",
  "scripts": {
    "build": "babel src --out-dir build --extensions '.ts,.tsx' --ignore 'src/**/*.test.tsx','src/setupTests.js'",
    "declarations": "tsc -b tsconfig.defs.json",
    "test": "jest --watch",
    "test-once": "jest",
    "prepublishOnly": "npm run test-once && npm run build && npm run declarations"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "shortid": "^2.2.16"
  },
  "peerDependencies": {
    "react": "^16.0.1",
    "react-dom": "^16.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.5",
    "@types/shortid": "^0.0.29",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^27.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ]
  }
}
